<div class=<%= div_class %>>
  <div class='col-md-1'></div>
  <div class='col-md-10'>
    <table class="table table-striped table-hover ">
      <h2 class='reservations-header'><%= header %></h2>
      <thead>
        <tr>
          <th width='50%'>Type</th>
          <th width='15%'>Start</th>
          <th width='15%'>End</th>
          <th width='10%'>Total</th>
          <th width='10%'>Options</th>
        </tr>
      </thead>
      <tbody>
        <% reservations.each do |reservation| %>
          <%= render partial: 'index_table_row', locals: {renter: current_user.renter, reservation: reservation} %>
        <% end %>
      </tbody>
    </table> 
  </div>
  <div class='col-md-1'></div>
</div>
