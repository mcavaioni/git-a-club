<script id="clubs-template" type="text/x-handlebars-template">
  <div class='col-md-12'>
    <table class="table table-striped table-hover ">
      <thead>
        <tr>
          <th width='23%'>Type</th>
          <th width='15%'>Brand</th>
          <th width='7%'>Gender</th>
          <th width='7%'>Hand</th>
          <th width='10%'>Head</th>
          <th width='13%'>Shaft</th>
          <th width='15%'>Condition</th>
          <th width='10%'>Options</th>
        </tr>
      </thead>
      <tbody>
        {{#each clubs as |club|}}
          <tr>
            <td width='23%'><a href={{url}} class='club-details-link' data-method='get' data-remote='true'>{{club_type}}</a></td>
            <td width='15%'>{{brand}}</td>
            <td width='7%'>{{male}}</td>
            <td width='7%'>{{righty}}</td>
            <td width='10%'>{{head_feature}}</td>
            <td width='13%'>{{shaft_stiffness}}</td>
            <td width='15%'>{{condition}}</td>
            <th width='10%'><a href={{url}} data-method='delete'>Deactivate</a></th>
          </tr>
        {{/each}}
      </tbody>
    </table> 
  </div>
</script>