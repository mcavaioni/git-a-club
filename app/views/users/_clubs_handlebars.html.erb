<script id="clubs-template" type="text/x-handlebars-template">
  <div class='col-md-12'>
    <table class="table table-striped table-hover ">
      <thead>
        <tr>
          <th width='23%'>Type</th>
          <th width='15%'>Brand</th>
          <th width='7%'>Gender</th>
          <th width='7%'>Hand</th>
          <th width='10%'>Head</th>
          <th width='13%'>Shaft</th>
          <th width='15%'>Condition</th>
          <th width='10%'>Options</th>
        </tr>
      </thead>
      <tbody id='club-table-body'>
        {{#each clubs as |club|}}
          <tr>
            <td width='23%' class='club-url'><a href={{url}} class='club-details-link' data-method='get' data-remote='true'>{{club_type}}</a></td>
            <td width='15%' class='club-brand'>{{brand}}</td>
            <td width='7%' class='club-gender'>{{male}}</td>
            <td width='7%' class='club-hand'>{{righty}}</td>
            <td width='10%' class='club-head'>{{head_feature}}</td>
            <td width='13%' class='club-shaft'>{{shaft_stiffness}}</td>
            <td width='15%' class='club-condition'>{{condition}}</td>
            <td width='10%' class='club-deactivate'><a href={{club_url}} data-method='delete' data-remote="true" class='user-page-cancel'>Deactivate</a></td>
          </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
</script>
