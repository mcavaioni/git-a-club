<button class="btn-to-update">Update Info</button>
 <div class="parent-js" >
<%= form_for @user, html: {class: "user_form "} , remote: true do |f| %>
   <%= f.hidden_field :user_id, value: @user.id %>
   <%= f.label "First Name" %>
   <%= f.text_field :first_name, class: "thisone" %>
   <%= f.label "Last Name" %>
   <%= f.text_field :last_name %>
   <%= f.label "Address" %>
   <%= f.text_field :address %>
   <%= f.label "Phone Number" %>
   <%= f.text_field :phone_number %>
   <%= f.submit  %>
   
</div>
 <% end %>