<script id="club_sets-template" type="text/x-handlebars-template">
  <div class='col-md-12'>
    <table class="table table-striped table-hover ">
      <thead>
        <tr>
          <th width='90%'>Description</th>
          <th width='10%'>Options</th>
        </tr>
      </thead>
      <tbody>
        {{#each club_sets as |club_set|}}
          <tr>
            <td width='90%' class='club-set-url'><a href={{url}} class='club-set-details-link' data-method='get' data-remote='true'>{{description}}</a></td>
            <td width='10%' class='club-set-deactivate'><a href={{club_set_url}} data-method='delete'>Deactivate</a></td>
          </tr>
        {{/each}}
      </tbody>
    </table> 
  </div>
</script>