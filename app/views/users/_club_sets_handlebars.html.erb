<script id="club_sets-template" type="text/x-handlebars-template">
  <div class='col-md-12'>
    <table class="table table-striped table-hover ">
      <thead>
        <tr>
          <th width='20%'>Name</th>
          <th width='20%'>Description</th>
          <th width='50%'>Summary</th>
          <th width='10%'>Options</th>
        </tr>
      </thead>
      <tbody>
        {{#each club_sets as |club_set|}}
          <tr>
            <td width='20%' class='club-set-url'><a href={{url}} class='club-set-details-link' data-method='get' data-remote='true'>{{name}}</a></td>
            <td width='20%' class='club-set-description'>{{description}}</td>
            <td width='50%' class='club-set-summary'>{{summary}}</td>
            <td width='10%' class='club-set-deactivate'><a href={{club_set_url}} data-method='delete'>Deactivate</a></td>
          </tr>
        {{/each}}
      </tbody>
    </table> 
  </div>
</script>