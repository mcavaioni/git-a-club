<div class="col-md-3 car btn btn-default listing-button">
  <h5><%= listing.listable.description%></h5>
  <p><strong>Type:</strong> <%= listing.listable_type %></p>
  <p><strong>From:</strong> <%= listing.start_date %></p>
  <input class='listing-availability' type=hidden name='listing-availability' value='<%=listing.availability%>'>
  <p><strong>To:</strong> <%= listing.finish_date %></p>
  <div class='calendar'></div>
  <p><strong>Price:</strong> <%= listing.price %></p>
  <%=link_to listing.id, listing %>
</div>
