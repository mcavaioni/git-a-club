<% if logged_in?  %>
  <li>
    <%= link_to 'My Rentals', renter_reservations_path(user.renter) %>
  </li>
  <li>
    <%= link_to 'Supply Clubs', user.supplier ? supplier_path(user.supplier) : new_supplier_path %>
  </li>
  <li>
    <%= link_to 'Profile', user %>
  </li>
  <li>
    <%=link_to 'Logout', logout_path, method: :delete %>
  </li>
<% else %>
  <li><%=link_to 'Login', login_path %></li><li><%=link_to 'Sign up', new_user_path %></li>
<% end %>
